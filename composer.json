{
  "name": "dsheiko/magentosearchquerybuilder",
  "type": "library",
  "description": "Tool to build search criteria query for Magento REST Web API",
  "keywords": ["Magento", "Search", "library", "REST"],
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Sheiko",
      "homepage": "http://dsheiko.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "peridot-php/leo": "^1.6",
    "peridot-php/peridot": "^1.19",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Dsheiko\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "test": "vendor/bin/peridot tests/"
  },
  "config": {
    "sort-packages": true
  }
}
